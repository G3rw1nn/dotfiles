{
  "$schema": "https://raw.githubusercontent.com/jetify-com/devbox/0.12.0/.schema/devbox.schema.json",
  "packages": [
    "kubectl@latest",
    "kubernetes-helm@latest",
    "teller@latest",
    "stow@latest",
    "terraform@latest",
    "k3sup@latest",
    "kubevirt@latest"
  ],
  "shell": {
    "init_hook": [
      "autoload -Uz compinit",
      "compinit",
      "source <(kubectl completion zsh)"
    ],
    "scripts": {}
  }
}
